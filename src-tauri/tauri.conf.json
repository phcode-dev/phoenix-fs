{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "",
        "beforeDevCommand": "npm run _serve-tauri",
        "devPath": "http://localhost:8081/test/",
        "distDir": "../test/",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "Phoenix File system",
        "version": "2.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": true,
            "fs": {
                "all": true,
                "scope": [
                    "$APPCONFIG/**",
                    "$APPDATA/**",
                    "$APPLOCALDATA/**",
                    "$APPCACHE/**",
                    "$APPLOG/**",
                    "$AUDIO/**",
                    "$CACHE/**",
                    "$CONFIG/**",
                    "$DATA/**",
                    "$LOCALDATA/**",
                    "$DESKTOP/**",
                    "$DOCUMENT/**",
                    "$DOWNLOAD/**",
                    "$EXE/**",
                    "$FONT/**",
                    "$HOME/**",
                    "$PICTURE/**",
                    "$PUBLIC/**",
                    "$RUNTIME/**",
                    "$TEMPLATE/**",
                    "$VIDEO/**",
                    "$RESOURCE/**",
                    "$APP/**",
                    "$LOG/**",
                    "$TEMP/**",
                    "**",
                    "**/**"
                ]
            }
        },
        "cli": {
            "description": "phoenix-fs test runner cli",
            "longDescription": "phoenix-fs test runner cli",
            "beforeHelp": "",
            "afterHelp": "",
            "args": [{
                "name": "quit-when-done",
                "short": "q"
            }]
        },
        "bundle": {
            "active": false,
            "category": "DeveloperTool",
            "copyright": "Copyright (c) 2022 - present Core.ai",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "fs.phcode",
            "longDescription": "Phoenix virtual file system over filer/ browser fs access/ tauri / phoenix web socket APIs.",
            "appimage": {
            },
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "localhost",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "shortDescription": "Phoenix virtual file system over filer/ browser fs access/ tauri / phoenix web socket APIs.",
            "targets": [
                "appimage",
                "nsis",
                "app",
                "dmg",
                "updater"
            ],
            "windows": {
                "digestAlgorithm": "sha256",
                "timestampUrl": "http://timestamp.comodoca.com"
            }
        },
        "security": {
            "csp": null
        },
        "windows": [
            {
                "fullscreen": false,
                "height": 768,
                "minHeight": 600,
                "resizable": true,
                "title": "Phoenix File System",
                "width": 1024,
                "minWidth": 800,
                "acceptFirstMouse": true
            }
        ]
    }
}